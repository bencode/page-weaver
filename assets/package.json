  },
  "devDependencies": {
    "bcd-react-webpack": "^10.0.0-beta.1",
    "eslint": "^6.8.0",
    "eslint-config-bcd-react": "^3.0.0",
    "stylelint": "^13.2.0",
    "stylelint-config-bcd": "^1.0.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "license": "MIT"
}
